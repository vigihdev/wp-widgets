{
    "name": "vigihdev/wp-widgets",
    "version": "1.0.1",
    "description": "Vigih Dev Wp Widgets",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "wordpress",
        "seo",
        "plugin",
        "vigihdev",
        "Assets"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Vigih Dev",
            "email": "vigihdev@gmail.com",
            "role": "developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "composer/installers": "^2.0",
        "symfony/filesystem": "^6.4",
        "yiisoft/html": "^3.11"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "WpWidgets\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0,
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "scripts": {
        "test": "phpunit",
        "cs-check": "phpcs --standard=PSR12 src/",
        "cs-fix": "phpcbf --standard=PSR12 src/",
        "server": "php -S localhost:5477 server.php"
    }
}
